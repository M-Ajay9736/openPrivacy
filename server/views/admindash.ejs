  <script type="text/javascript">
    let httpRequest = (_link, _json, successCallback, failCallback=(a,b,c) => {console.log("error")}) => {
    $.ajax({
        url: _link,
        dataType: 'json',
        type: 'post',
        contentType: 'application/json',
        data: JSON.stringify(_json),
        processData: false,
        success: successCallback,
        error: failCallback
    })
}
</script>

